{% if node.field_banner_principal.0 %}
  <div>
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
      <ul class="carousel-indicators G-tabs circular G-txt--cen G-pos--absoluto" style="bottom: 16px;">
        {% for key,item in node.field_banner_principal %}
          <li data-target="#carouselExampleIndicators" data-slide-to="{{ key }}" class="{% if(key == 0) %}  active {% endif %}"></li>
        {% endfor %}
      </ul>
      <div class="carousel-inner">
        {% for key,item in node.field_banner_principal %}
          <div class="carousel-item {% if(key == 0) %}  active {% endif %}">
            <div class="E-fondo-norepeat E-espacio-cabecera G-fondo--full G-flex G-dis-v--middle G-dis-h--cen G-pos--relativo" style="background-image:url('{{ file_url(item.entity.field_media_image_2.entity.fileuri) }}');height: 360px;background-position: center bottom;">
              <div class="G-flex G-flex-h--repartidos E-max--580 G-margen--auto" style="padding: 50px 20px;">
                {% if item.entity.field_descripcion.value %}
                  <div style="line-height: 1;" class="G-max--100 G-txt--xxgigante G-txt--negrita G-color--blanco G-sombra-txt--si-s G-txt--cen E-p-nomargen E-titulo-resp">
                    {{ item.entity.field_descripcion.value|raw }}
                  </div>
                {% endif %}
              </div>
            </div>
          </div>
        {% endfor %}
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
  </div>
{% endif %}

<div class="E-fondo-norepeat G-fondo--full G-max--100" style="background-image: linear-gradient(134deg, rgba(255,255,255,0.5) 0%, rgba(255,255,255,0.63) 28%, rgba(255,255,255,0.85) 59%, #FFFFFF 100%), url('{{ file_url(node.field_imagen_de_fondo.entity.fileuri) }}');">
